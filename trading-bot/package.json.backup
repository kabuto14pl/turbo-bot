{
  "name": "trading-bot",
  "version": "1.0.0",
  "description": "A trading bot framework with core logic, infrastructure, and interface layers.",
  "main": "autonomous_trading_bot.js",
  "scripts": {
    "start": "ts-node main.ts",
    "start:autonomous": "ts-node autonomous_trading_bot.ts",
    "start:autonomous:prod": "NODE_ENV=production ts-node autonomous_trading_bot.ts",
    "start:autonomous:demo": "NODE_ENV=demo BOT_MODE=demo ts-node autonomous_trading_bot.ts",
    "start:enterprise": "ts-node enterprise_trading_bot_system.ts",
    "start:enterprise:prod": "NODE_ENV=production ts-node enterprise_trading_bot_system.ts",
    "start:enterprise:dev": "NODE_ENV=development LOG_LEVEL=debug ts-node enterprise_trading_bot_system.ts",
    "start:dashboard": "ts-node -e \"import('./core/dashboard/enterprise_dashboard_system').then(m => new m.EnterpriseDashboardSystem().start())\"",
    "start:alerts": "ts-node -e \"import('./core/alerts/enterprise_alert_coordination_system').then(m => new m.EnterpriseAlertCoordinationSystem().start())\"",
    "start:risk": "ts-node -e \"import('./core/risk/enterprise_risk_management_system').then(m => new m.EnterpriseRiskManagementSystem().start())\"",
    "start:production": "NODE_ENV=production ts-node production_main.ts",
    "start:demo": "NODE_ENV=demo ts-node production_main.ts",
    "start:production:confirmed": "NODE_ENV=production PRODUCTION_CONFIRMED=true ENABLE_REAL_TRADING=true ts-node production_main.ts",
    "test": "jest",
    "test:enterprise": "jest --testPathPattern=enterprise",
    "test:integration": "ts-node main.ts",
    "build": "tsc",
    "build:enterprise": "tsc enterprise_trading_bot_system.ts --outDir dist",
    "build:production": "tsc production_main.ts --outDir dist",
    "security:audit": "npm audit --audit-level=moderate",
    "clean": "rimraf dist",
    "start:monitoring": "docker-compose -f analytics/docker-compose.yml up -d",
    "stop:monitoring": "docker-compose -f analytics/docker-compose.yml down",
    "metrics": "ts-node analytics/metrics/metrics_server.ts",
    "dashboard:api": "ts-node dashboard_api.ts",
    "optimize:all": "ts-node tools/optimize_all_strategies.ts",
    "optimize:max-profit": "ts-node max_profit_optimizer_ai.ts",
    "start:live-demo": "ts-node max_profit_optimizer_ai.ts --live-demo",
    "test:max-profit": "ts-node test_max_profit_optimizer.ts",
    "test:optimization-only": "ts-node test_max_profit_optimizer.ts --optimization-only",
    "test:live-only": "ts-node test_max_profit_optimizer.ts --live-only"
  },
  "dependencies": {
    "@tensorflow/tfjs-data": "^4.22.0",
    "@tensorflow/tfjs-layers": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@tensorflow/tfjs-vis": "^1.5.1",
    "@types/compression": "^1.8.1",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.13",
    "@types/python-shell": "^0.4.2",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^0.21.1",
    "compression": "^1.8.1",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.1",
    "duckdb": "^1.3.2",
    "events": "^3.3.0",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "grafana-dash-gen": "^3.1.0",
    "helmet": "^6.2.0",
    "ioredis": "^5.7.0",
    "js-yaml": "^4.1.0",
    "kafkajs": "^2.2.4",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "open": "^10.2.0",
    "piscina": "^4.9.2",
    "prom-client": "^14.0.1",
    "python-shell": "^5.0.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@types/cors": "^2.8.19",
    "@types/csv-stringify": "^1.4.3",
    "@types/express": "^4.17.23",
    "@types/express-rate-limit": "^6.0.2",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "csv-parse": "^6.0.0",
    "csv-stringify": "^6.6.0",
    "eslint": "^9.34.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/trading-bot.git"
  },
  "keywords": [
    "trading",
    "bot",
    "finance",
    "investment"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/trading-bot/issues"
  },
  "homepage": "https://github.com/yourusername/trading-bot#readme"
}
